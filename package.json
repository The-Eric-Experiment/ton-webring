{
  "name": "theoldnet-webring",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ENVIRONMENT=development ts-node-dev ./src/server.ts",
    "dev": "ENVIRONMENT=development ts-node-dev --respawn ./src/server.ts",
    "tsc": "tsc",
    "build": "rimraf dist/ && yarn tsc",
    "start:prod": "ENVIRONMENT=production node ./dist/server.js",
    "start:prod2": "ENVIRONMENT=production ts-node-dev ./src/server.ts",
    "deploy": "git pull && yarn build && pm2 restart server"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "luxon": "^2.0.1",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.2",
    "uglify-js": "^3.14.0",
    "vash": "^0.13.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.7",
    "@types/express-useragent": "^1.0.1",
    "@types/luxon": "^1.27.1",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@types/uglify-js": "^3.13.1",
    "@types/yaml": "^1.9.7",
    "ts-node-dev": "^1.0.0-pre.48",
    "typescript": "^3.9.5"
  }
}
